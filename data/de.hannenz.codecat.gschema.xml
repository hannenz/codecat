<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/de/hannenz/codecat/" id="de.hannenz.codecat">
		<key name="show-error-notifications" type="b">
			<default>true</default>
			<summary>Show notifications on error</summary>
			<description>Show notifications on error</description>
		</key>
		<key name="show-success-notifications" type="b">
			<default>true</default>
			<summary>Show notifications on success</summary>
			<description>Show notifications on success</description>
		</key>
		<key name="transition" type="s">
			<choices>
				<choice value="none"/>
				<choice value="crossfade"/>
				<choice value="slide-left-right"/>
			</choices>
			<default>'slide-left-right'</default>
			<summary>Transition</summary>
			<description>The transition to use when switching tabs.</description>
		</key>
	</schema>
</schemalist>
